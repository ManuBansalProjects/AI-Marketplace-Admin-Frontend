{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://ai-marketplace-admin-backend.vercel.app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
